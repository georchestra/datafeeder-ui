    "@angular/cli": "~18.2.0",
    "@angular/compiler-cli": "18.2.13",
    "@ngrx/effects": "18.1.1",
    "@ngrx/router-store": "18.1.1",
    "@ngrx/store": "18.1.1",
    "@ngrx/store-devtools": "18.1.1",
    "@nx/eslint-plugin": "20.2.2",
    "@nx/jest": "20.2.2",
    "@schematics/angular": "18.2.12",
    "@types/chroma-js": "^2.1.3",
    "@types/geojson": "^7946.0.7",

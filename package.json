    "@ngrx/store": "16.0.1",
    "@ngrx/store-devtools": "16.0.1",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/linter": "16.6.0",
    "@schematics/angular": "16.1.6",
    "@types/chroma-js": "^2.1.3",
    "@types/geojson": "^7946.0.7",

    "@geospatial-sdk/geocoding": "^0.0.5-alpha.2",
    "@messageformat/core": "^3.0.1",
    "@ngx-translate/core": "~14.0.0",
    "@ngx-translate/http-loader": "~7.0.0",
    "@nx/angular": "16.6.0",
    "chroma-js": "^2.1.2",
    "moment": "^2.29.4",
